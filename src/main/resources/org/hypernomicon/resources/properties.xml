  <!--This removes the menu bar entirely if set to false-->
  <ShowMenubar value="false"/>
  <!--This removes the button bar entirely if set to false-->
  <ShowButtons value="false"/>
  <!--This removes the display options bar entirely if set to false-->
  <ShowDisplayoptions value="true"/>
  <!--This removes the navigation bar entirely if set to false-->
  <ShowNavigationbar value="false"/>
  <!--This removes the side tab bar entirely if set to false-->
  <ShowSidetabbar value="true"/>
  <!--The integer RGB value for the highlight color-->
  <highlightBoxColor value="-16777216"/>
  <!--The integer RGB value for the highlighted text color-->
  <highlightTextColor value="16750900"/>
  <!--Flag to replace document text colors with user defined value-->
  <replaceDocumentTextColors value="false"/>
  <!--Integer RGB value for replace text colors-->
  <vfgColor value="0"/>
  <!--Integer RGB value to replace document page color-->
  <vbgColor value="16777215"/>
  <!--All color values (R,G and B), must be under this value in order to change text color-->
  <TextColorThreshold value="255"/>
  <!--Flag to replace the background color of the display pane-->
  <replacePdfDisplayBackground value="false"/>
  <!--Color to use as display pane color if flag is set-->
  <pdfDisplayBackground value="16777215"/>
  <!--Allows text color change to also change color of shapes and line art-->
  <changeTextAndLineart value="false"/>
  <!--Transparency value to be used for the highlight box color-->
  <highlightComposite value="0.35"/>
  <!--This overrides the highlight box color and inverts the color of anything within the highlight area-->
  <invertHighlights value="false"/>
  <!--Flag to open last document upon openning the viewer-->
  <openLastDocument value="false"/>
  <!--Page to open last document to-->
  <lastDocumentPage value="1"/>
  <!--The inset of the page in the play area-->
  <pageInsets value="25"/>
  <!--The length of the tabbed pane when it has been collapsed-->
  <sideTabBarCollapseLength value="32"/>
  <!--The length of the tabbed pane when it has been expanded-->
  <sideTabBarExpandLength value="190"/>
  <!--Keep side tab bar consitent across multiple files, overrides startSideTabOpen on file open-->
  <consistentTabBar value="false"/>
  <!--Flag to allow for the right click menu to be used in the viewer-->
  <allowRightClick value="true"/>
  <!--Flag to allow the mouse scroll wheel to zoom in / out of display-->
  <allowScrollwheelZoom value="true"/>
  <!--Flag to set if the properties file can be modified using the preferences window-->
  <readOnly value="true"/>
  <!--Flag to use enhanced viewer mode-->
  <enhancedViewerMode value="true"/>
  <!--Flag to use enhanced facing mode-->
  <enhancedFacingMode value="true"/>
  <!--Text to use in the window title-->
  <windowTitle value=""/>
  <!--Flag to control if we requestion confirmation to close the viewer-->
  <confirmClose value="false"/>
  <!--Location where the icons to be used by the viewer are stored-->
  <iconLocation value="/org/jpedal/examples/viewer/res/new/"/>
  <!--Flag to control if we show a message when entering page flow mode-->
  <showpageflowmessage value="true"/>
  <!--Specify a default printer to use when first opening the print dialog-->
  <defaultPrinter value=""/>
  <!--Flag to output additional printer / printing info-->
  <debugPrinter value="false"/>
  <!--Default printing DPI-->
  <defaultDPI value="600"/>
  <!--Default printing page size-->
  <defaultPagesize value=""/>
  <!--List of printers to ignore-->
  <printerBlacklist value=""/>
  <!--Flag to allow the use of hinting for true type fonts-->
  <useHinting value="false"/>
  <!--Voice name to be used for text to speech functionality-->
  <voice value="kevin16(general domain)"/>
  <!--Flag to turn on previews in single page mode when scrolling-->
  <previewOnSingleScroll value="true"/>
  <!--Flag to show the bounding box of the mouse selection-->
  <showMouseSelectionBox value="false"/>
  <!--Flag if we should separate the cover of document when in facing mode-->
  <separateCoverOn value="true"/>
  <!--Flag to set the JavaFX transition type-->
  <transitionType value="None"/>
  <!--Flag to track the users scaling between sessions-->
  <trackScaling value="false"/>
  <!--Scaling value to use on viewer start up, used/modified when tracking between sessions-->
  <startScaling value="Fit Page"/>
  <!--Flag to track the users display mode between sessions-->
  <trackView value="false"/>
  <!--Display Mode value to use on viewer start up, used/modified when tracking between sessions-->
  <startView value="1"/>
  <!--Flag to track the if the side bar tab is open between sessions-->
  <trackSideTabOpen value="false"/>
  <!--Flag if side tab bar should be open on viewer start up and file opening, used/modified when tracking between sessions-->
  <startSideTabOpen value="false"/>
  <!--Flag to track the currently selected tab on side bar between sessions-->
  <trackSelectedSideTab value="false"/>
  <!--Side tab bar to be selected on viewer start up / file openning, used/modified when tracking between sessions-->
  <startSelectedSideTab value=""/>
  <!--Flag to track the viewer window size between sessions-->
  <trackViewerSize value="false"/>
  <!--Viewer width to use on viewer start up, used/modified when tracking between sessions-->
  <startViewerWidth value="960"/>
  <!--Viewer height to use on viewer start up, used/modified when tracking between sessions-->
  <startViewerHeight value="526"/>
  <!--Flag to track the side tab bar expanded width between sessions-->
  <trackSideTabExpandedSize value="false"/>
  <!--Allow search result list to update during search (SWING VERSION ONLY)-->
  <updateResultsDuringSearch value="true"/>
  <!--Allow thin lines (width less than 1) to be made wider and clearly visible-->
  <enhanceFractionalLines value="true"/>
</properties>