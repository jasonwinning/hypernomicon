<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="200.0" minWidth="150.0" prefHeight="568.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hypernomicon.dialogs.ParentConceptDlgCtrlr">
   <children>
      <BorderPane layoutX="165.0" layoutY="113.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="tv" editable="true" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="75.0" sortable="false" text="Parent Concept(s)" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
         <bottom>
            <BorderPane BorderPane.alignment="CENTER">
               <right>
                  <HBox alignment="BASELINE_CENTER" spacing="4.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button defaultButton="true" maxHeight="25.0" maxWidth="60.0" minHeight="25.0" minWidth="60.0" mnemonicParsing="false" onAction="#btnOkClick" prefHeight="25.0" prefWidth="60.0" text="OK" />
                        <Button cancelButton="true" maxHeight="25.0" maxWidth="60.0" minHeight="25.0" minWidth="60.0" mnemonicParsing="false" onAction="#btnCancelClick" prefHeight="25.0" prefWidth="60.0" text="Cancel" />
