<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="500.0" prefWidth="1076.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hypernomicon.settings.EditWebButtonsDlgCtrlr">
  <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="319.0" layoutY="96.0" AnchorPane.bottomAnchor="32.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <AnchorPane>
      <GridPane fx:id="gpMain" prefHeight="73.0" prefWidth="1074.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES"/>
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES"/>
        </rowConstraints>
      </GridPane>
    </AnchorPane>
  </ScrollPane>
  <Label layoutX="14.0" layoutY="475.0"
         text="Click field button to add field to pattern. If checked, then the field must be present for the pattern in question to be used."
         AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="14.0"/>
  <Button fx:id="btnAdd" layoutX="829.0" layoutY="471.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="93.0"
          text="Add Pattern" AnchorPane.bottomAnchor="4.0" AnchorPane.rightAnchor="154.0"/>
  <Button defaultButton="true" layoutX="927.0" layoutY="471.0" mnemonicParsing="false" onAction="#btnOkClick"
          prefHeight="25.0" prefWidth="70.0" text="OK" AnchorPane.bottomAnchor="4.0" AnchorPane.rightAnchor="79.0"/>
  <Button cancelButton="true" layoutX="1002.0" layoutY="471.0" mnemonicParsing="false" onAction="#btnCancelClick"
          prefHeight="25.0" prefWidth="70.0" text="Cancel" AnchorPane.bottomAnchor="4.0" AnchorPane.rightAnchor="4.0"/>
</AnchorPane>
